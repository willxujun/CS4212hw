/* method overloading - error */
class Main {
    Void main() {
        A a;
        Int value;
        a = new A();
        value = a.getValue(1,2,2);
        value = a.getValue(3);
        value = a.getValue(4,true);
        return;
    }
}

class A {
    Int value;
    B b;
    
    B getB() {
        return b;
    }

    Int getValue(Int a, Int b, Int c) {
        this.value = -(a*b-c);
        return value;
    }
    Int getValue(Int a) {
        return a;
    }
    Int getValue(Int a, Bool b) {
        if(b) {
            return a;
        } else {
            return 0;
        }
    }
    
}

class B {
    C c;
    
    C getC() {
        return c;
    }
}

class C {
    A a;
    D d;

    A getA() {
        return a;
    }

    D getD() {
        return d;
    }
}

class D {
    A a;

    A getA() {
        return this.a;
    }
}
